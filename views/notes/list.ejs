<h3><a href="/notes/add" class="btn btn-info btn-sm"><i class="fa fa-plus"></i>Add New</a></h3>
<table class="table table-bordered">
    <tr>
        <th>DATE</th>
        <th>NOTE</th>
        <th>ACTION</th>
    </tr>
    <% for(var i=0; i < list.length; i++) { %>
    <tr>
        <td><%= list[i].DT %></td>
        <td><%= list[i].NOTE %></td>
        <td>
            <a href="/notes/<%= list[i]._id %>" class="text-info"><i class="fas fa-pencil-alt"></i></a> &nbsp; | &nbsp;
            <a href="/notes/delete/<%= list[i]._id %>" onclick="return confirm('Are you sure to delete this record?');" class="text-danger"><i class="far fa-trash-alt"></i></a>
        </td>
    </tr>
    <% } %>
</table>