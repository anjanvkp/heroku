<h3><a href="/lic/add" class="btn btn-info btn-sm" id="btnADD"><i class="fa fa-plus"></i>Add New</a> Lic List</h3>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>CANAME</th>
        <th>CA NO</th>
        <th>LOA NO</th>
        <th>START DATE</th>
        <th>END DATE</th>
        <th>VALUE</th>
        <th>ACTION</th>
    </tr>
    <% for(var i=0; i < list.length; i++) { %>
    <tr>
        <td><%= list[i].rid %></td>
        <td><%= list[i].CANAME %></td>
        <td><%= list[i].CANO %></td>
        <td><%= list[i].LOANO %></td>
        <td><%= list[i].SDATE %></td>
        <td><%= list[i].EDATE %></td>
        <td><%= list[i].VAL %></td>
        <td>
            <a href="/lic/<%= list[i]._id %>"><i class="fas fa-pencil-alt"></i></a> &nbsp; | &nbsp;
            <a href="/lic/delete/<%= list[i]._id %>" onclick="return confirm('Are you sure to delete this record?');"><i class="far fa-trash-alt"></i></a>
        </td>
    </tr>
    <% } %>
</table>